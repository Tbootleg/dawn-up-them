<link rel="stylesheet" href="{{ 'section-main-page.css' | asset_url }}" media="print" onload="this.media='all'">
<link rel="stylesheet" href="{{ 'component-rte.css' | asset_url }}" media="print" onload="this.media='all'">

<noscript>{{ 'section-main-page.css' | asset_url | stylesheet_tag }}</noscript>
<noscript>{{ 'component-rte.css' | asset_url | stylesheet_tag }}</noscript>

{%- style -%}
.section-{{ section.id }}-padding {
  padding-top: {{ section.settings.padding_top | times: 0.75 | round: 0 }}px;
  padding-bottom: {{ section.settings.padding_bottom | times: 0.75 | round: 0 }}px;
}

@media screen and (min-width: 750px) {
  .section-{{ section.id }}-padding {
    padding-top: {{ section.settings.padding_top }}px;
    padding-bottom: {{ section.settings.padding_bottom }}px;
  }
}
{%- endstyle -%}

<div class="screenprinting-service-content page-width page-width--narrow section-{{ section.id }}-padding">
  <h1 class="main-page-title page-title h0">
    {{ page.title | escape }}
  </h1>
  <div class="rte">
    <div class="service-page-tagline--container">
      <h1 class="service-page-tagline--tagline">Everyone Loves Swag</h1>
    </div>
    <div class="service-page-content-box service-page-intro-box">
      <div class="service-page-intro-box--inner">
        <div class="service-page-content--image">
          <img class="" src="https://cdn.shopify.com/s/files/1/0621/6223/9651/files/jibitz-no-background.webp?v=1676064477">
        </div>
        <div class="service-page-content--text">
          <h3>Endless Options</h3>
          <p>We have a massive selection of promotional products ready to have your logo on them. This is a great way to show off your brand and promote interest from new potential customers.</p>
          <p>Hit us up and we will help you find the perfect product for your needs.</p>
        </div>
      </div>
    </div>
  </div>
</div>
  

  {% schema %}
  {
    "name": "t:sections.main-page.name",
    "tag": "section",
    "class": "section",
    "settings": [
    {
      "type": "header",
      "content": "t:sections.all.padding.section_padding_heading"
    },
    {
      "type": "range",
      "id": "padding_top",
      "min": 0,
      "max": 100,
      "step": 4,
      "unit": "px",
      "label": "t:sections.all.padding.padding_top",
      "default": 36
    },
    {
      "type": "range",
      "id": "padding_bottom",
      "min": 0,
      "max": 100,
      "step": 4,
      "unit": "px",
      "label": "t:sections.all.padding.padding_bottom",
      "default": 36
    }
    ]
  }
  {% endschema %}